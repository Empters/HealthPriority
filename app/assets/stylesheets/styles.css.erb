/* Generic styles */
body {
    padding: 0px;
    margin: 0px;
}

.nopadding {
    padding: 0;
}

.nomargin {
    margin: 0;
}

.left {
    float: left;
}

.right {
    float: right;
}

.button {
    clear: both;
    padding: 10px 20px;
    color: #FFFFFF;
}

.button:hover {
    cursor: pointer;
}

/* Generic styles End */

/* Common styles */
.top-bar-wrapper,  .button.green:hover{
    background: #f79114;
}

.top-bar-text.phone {
    font-style: italic;
    font-size: 14px;
}

.top-bar-text, .top-bar-container, .main-nav-tab, .button, .item-title, .products-count, .category-name, .category-item, .footer-main-text,.footer-text-title, .footer-text, .categories-list>li, .total-price, .dropdown-menu, #search_category, .phone-nav-title  {
    font-family: 'Open Sans';
}

.top-bar-text, .top-bar-container, .main-nav-tab, .button, .item-title, .main-nav-tab, .title-banner, .page.active, .page:hover, .footer-main-text, .cart-amount, .total-price, .phone-nav-title {
    color: #FFFFFF;
}

.main-nav-wrapper, .title-banner, .footer, .button.green, .button.orange:hover, .page.active, .page:hover,.rsMinW .rsBullet span, .phone-nav {
    background: #69a820;
}

.item-title, .total-price {
    color: #3e7808;
}

.item-title, .category-name, .title-banner, .category-name, .top-bar-text, .total-price, .phone-nav-title {
    text-transform: uppercase;
}

.pager-wrapper, .page {
    border-right: solid #BBD1A1 1px;
    border-bottom: solid #BBD1A1 1px;
    border-left: solid #BBD1A1 1px;
}

.products-count, .page {
    color: #8A8A8A;
}

.item-image, .fb_top, .gallery-item, .item-image, .rsMinW.rsHor .rsArrowIcn, .logo, .cart-amount, .cart, .search-image, .send-email-button, .star {
    background-size: contain;
    background-position: center;
    background-repeat: no-repeat;
}
/* Common styles End */

.top-bar-wrapper {
    position: relative;
}

.top-bar-container, .pager-container {
    font-size: 0px;
    letter-spacing: 0px;
    word-spacing: 0px;
}

.top-bar-text {
    padding: 5px 0px;
    font-size: 12px;
}

.top-bar-text:hover {
    color: #FFFFFF;
    text-decoration: none;
}

.top-bar-tab {
    display: inline-block;
    padding-right: 20px;
    padding-left: 20px;
    border-right: solid #FFFFFF 1px;
}

.top-bar-tab.first-tab {
    border-left: solid #FFFFFF 1px;
}

.top-bar-tab:hover {
    text-decoration: underline;
    cursor: pointer;
}

.main-nav-tab {
    display: inline-block;
    padding: 10px;
}

.fb_top {
    display: inline-block;
    height: 15px;
    width: 10px;
    vertical-align: middle;
}

.main-nav-tab:hover, .navbar-header:hover, .nav>li>a:hover, .nav>li>a:focus {
    background: #5a9812;
    cursor: pointer;
}

.phone-nav-title {
    font-size: 16px;
}

.navbar-header {
    text-align: center;
}

.navbar-toggle {
    border:none;
    float:none;
}

.navbar-default .navbar-toggle .icon-bar {
    background: #FFFFFF;
}

.navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus {
    background: none;
}

.phone-nav-title, .navbar-toggle {
   display: inline-block;
}

.title-banner {
    padding: 10px;
}

.search-section-container {
    margin: 15px 0px;
}

.search-section, .logo {
    height: 80px;
}

.logo {
    background-image: url("<%= image_path "logo.png" %>");
    display: block;
}

.search-section {
    padding: 20px 40px;
    border-right: solid #ACACAC 1px;
    border-left: solid #ACACAC 1px;
}

.search-container, .email-container {
    position: relative;
    height: 40px;
    border: solid #F79114 1px;
}

.search-dropdown-container, .search-input-container {
    height: 100%;
    display: inline-block;
}

.search-dropdown-container {
    width: 25%;
    color: #bdbdbd;
}

#search_category {
    height: 100%;
    padding-top: 10px;
    padding-left: 10px;
    border-right: solid #F79114 1px;
    cursor:pointer;
}

.dropdown-menu {
    margin: 0;
    padding:0;
    border-radius: 0;
    background-color: #FFFFFF;
    border: solid #F79114 1px;
}

.dropdown-menu li {
    padding: 10px;
    border-bottom: solid #F79114 1px;
}

.dropdown-menu li:hover {
    background:  #F79114;
    color: #FFFFFF;
    cursor:pointer;
}

.search-input-container {
    width: 60%;
    padding-left: 10px;
}

.search-input, .email-input {
    height: 100%;
    width: 100%;
    border: none;
}

.search-input:focus, .email-input:focus {
    outline: 0;
}

.search-image {
    width: 15%;
    height: 100%;
    background-image: url("<%= image_path "search.png" %>");
    background-size: inherit;
}

.cart-amount {
    padding-right: 4px;
    text-align: center;
    vertical-align: text-bottom;
    background-image: url("<%= image_path "items-count.png" %>");
}

.cart {
    background-image: url("<%= image_path "cart.png" %>");
    display: block;
}

.cart-amount, .cart {
    display: inline-block;
    width: 30px;
    height: 20px;
}

.total-price {
    margin-top: 10px;
    font-weight: 600;
}

.total {
    font-size: 12px;
    font-weight: 500;
}

.price {
    font-size: 18px;
}

.royalSlider {
    width: 100%;
}

.rsMinW, .rsMinW .rsOverflow, .rsMinW .rsSlide, .rsMinW .rsVideoFrameHolder, .rsMinW .rsThumbs {
    background: transparent;
}

.rsMinW .rsBullets {
    right: 50%;
    overflow: visible;
    padding-top: 0px;
    bottom: 30px;
}

.rsMinW .rsBullet {
    width: 10px;
    height: 10px;
    padding:10px;
}

.rsMinW .rsBullet span {
    width: 15px;
    height: 15px;
}


@media screen and (min-width: 1200px) {
    .gallery-item {
        height: 360px;
    }
}

@media screen and (max-width: 1200px) {
    .gallery-item {
        height: 300px;
    }
}

@media screen and (max-width: 950px) {
    .gallery-item {
        height: 220px;
    }
}

.rsMinW .rsArrowLeft {
    left: -10px;
}

.rsMinW .rsArrowRight {
    right: -10px;
}

.rsMinW .rsArrowRight, .rsMinW .rsArrowLeft {
    top: 45%;
}

.rsMinW .rsArrowRight .rsArrowIcn {
    background: url("<%= image_path "arrow-right.png" %>") no-repeat !important;
}

.rsMinW .rsArrowLeft .rsArrowIcn {
    background: url("<%= image_path "arrow-left.png" %>") no-repeat !important;
}
.rsMinW.rsHor .rsArrowIcn {
    margin: 0;
    background-position: 0px;
    background-size: contain !important;
}

.rsMinW .rsArrow, .rsMinW.rsHor .rsArrowIcn {
    width: 36px;
    height: 36px;
}

.content-container {
    padding: 20px 0px;
    border-right: solid #BBD1A1 1px;
    border-bottom: solid #BBD1A1 1px;
    border-left: solid #BBD1A1 1px;
}

.categories_and_products_container {
    margin: 40px 0px;
}

.categories-list {
    list-style-type: none;
    padding: 0px;
    margin: 0px;
}

.categories-list>li {
    margin: 5px 40px;
    font-size: 14px;
}

.categories-list>li>a {
    text-decoration: none !important;
    color: #7A7A7A !important;
}

.categories-list>li>a:hover {
    color: #f79114 !important;
}

.item-container {
    margin: 10px;
    padding: 0px 20px;
    border-right: solid #ACACAC 1px;
}

.item-container.last-item {
    border: none;
}

.item-image {
    display: block;
    height: 100px;
}

.item-image:hover {
    background-color: transparent;
}

.item-title {
    margin: 20px;
    font-size: 16px;
    font-weight: 600;
}

.item-price {
    margin-bottom: 10px;
    font-size: 16px;
    font-weight: 700;
    color: #5d5d5d;
}

.rating, .star-rating {
    width: 100px;
    height: 15px;
}

.star {
    width: 15px;
    height: 15px;
    display: inline-block;
    background-image: url("<%= image_path "product-page-star-inactive.png" %>");
}

.star.active {
    background-image: url("<%= image_path "product-page-star-active.png" %>");
}

.page {
    display: inline-block;
    font-size: 15px;
}

.page.active, .page:hover {
    cursor: pointer;
}

.products-count, .page {
    padding: 5px 10px;
}

.category-name {
    margin: 20px 0px;
    font-size: 16px;
    color: #3e7808;
}

.category-text, .category-item {
    font-style: italic;
    font-size: 14px;
    color: #7C7C7C !important;
}

.category-item {
    display: block;
    margin-top: 5px;
    text-decoration: none !important;
}

.category-item:hover, .categories-list>li>a:hover  {
    text-decoration: underline !important;
    background: none;
}

.footer {
    margin-top: 40px;
    padding: 50px;
}

.footer-main-text {
    font-size: 14px;
}

.footer-text-title {
    font-size: 12px;
}

.footer-text {
    font-size: 11px;
}

.footer-text, .footer-text-title {
    color: #CDDEB6;
}

.login-modal .modal-header {
        background: none repeat scroll 0 0 #f79114;
}

.login-links {
    float: left;
    text-align: left;
}

.login-links > a {
    color: #7a7a7a !important;
    text-decoration: none !important;
}

.top-bar-tab > a {
    color: #ffffff;
    text-decoration: none;
}

.email-container {
    margin-top: 20px;
}

.email-input {
    width: 80%;
}

.send-email-button {
    width: 20%;
    height: 100%;
    cursor: pointer;
    border-left: solid #F79114 1px;
    background-image: url("<%= image_path "arrow.png" %>");
}