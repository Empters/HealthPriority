<div class="container-background">
  <div class="container main-content">
    <div class="path">
      <%= render_breadcrumbs :separator => ' / ' %>
    </div>
    <div class="row">
      <div class="col-md-12">
        <div class="cart-title"><%= t('sitemap') %></div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <%= t('products')%>
        <%= link_to t('products'), products_path, :title => 'Visit Products' %>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <%= t('shopping_cart')%>
        <%= link_to t('shopping_cart'), shopping_carts_path, :title => 'Visit Shopping Cart' %>
