    <div class="row">
      <div class="col-md-12">
        <%= t('about_us')%>
        <%= link_to t('about_us'), about_us_path, :title => 'Visit Special Offers' %>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <%= t('our_partners')%>
